{
    "log": {
        "loglevel": "none"
    },
    "inbounds": [{
            "port": "$PORT",
            "protocol": "vmess",
            "settings": {
                "clients": [{
                    "id": "$UUID"
                }]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/vmess"
                }
            }
        },
        {
            "port": "$PORT",
            "protocol": "VLESS",
            "settings": {
                "clients": [{
                    "id": "$UUID",
                    "alterId": 0
                }],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/app"
                }
            }
        }
    ],
    "outbounds": [{
        "protocol": "freedom"
    }]
}